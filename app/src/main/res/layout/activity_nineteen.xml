<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/bg_gradient2"
    android:layout_height="match_parent"
    tools:context=".Nineteen">

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="97dp"
        android:layout_toEndOf="@+id/imageView20"
        android:text="Special App"
        android:textColor="#000000"
        android:textSize="28sp" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="100dp"
        android:layout_marginTop="85dp"
        app:srcCompat="@drawable/pro" />

    <Button
        android:id="@+id/button42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="260dp"
        android:textStyle="bold"
        android:layout_marginLeft="260dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/rounded_button3"
        android:drawableStart="@drawable/ic_baseline_arrow_back_24"
        android:drawableLeft="@drawable/ic_baseline_arrow_back_24"
        android:padding="12dp"
        android:text="Back" />

    <Button
        android:id="@+id/button43"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="250dp"
        android:background="@drawable/rounded_button7"
        android:text="Calculator"
        android:textColor="#000000"
        android:textSize="16sp" />

    <Button
        android:id="@+id/button44"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button43"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="75dp"
        android:background="@drawable/rounded_button7"
        android:text="Text To Speech"
        android:textColor="#000000"
        android:textSize="16sp" />

    <Button
        android:id="@+id/button45"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button44"
        android:layout_alignParentStart="true"
        android:layout_marginStart="75dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/rounded_button7"
        android:text="Vibrator"
        android:textColor="#000000"
        android:textSize="16sp" />

    <Button
        android:id="@+id/button46"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button45"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="75dp"
        android:background="@drawable/rounded_button7"
        android:text="Quiz App"
        android:textColor="#000000"
        android:textSize="16sp" />
</RelativeLayout>